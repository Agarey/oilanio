475432.jpg'\r\n        ]\r\n        let imageIndex = -1;\r\n        return (\r\n            <div style={{width: '100%'}}>\r\n                <Slider {...settings} style={{height: '100%'}} className={styles.slider}>\r\n                    {props.stocks.map(elem => {\r\n                        imageIndex++;\r\n                        if(imageIndex === 5){\r\n                            imageIndex = 0;\r\n                        }\r\n                        let lol = imageIndex;\r\n                        return (<StockCard \r\n                            className={styles.sliderItem}\r\n                            lol={1} \r\n                            image={stockImages[imageIndex]} \r\n                            stock={elem}/>)\r\n                    })}\r\n                </Slider>\r\n                <div className=\"styles.slider styles.center\">\r\n                <div>\r\n                    <h3>1</h3>\r\n                </div>\r\n                <div>\r\n                    <h3>2</h3>\r\n                </div>\r\n                <div>\r\n                    <h3>3</h3>\r\n                </div>\r\n                <div>\r\n                    <h3>4</h3>\r\n                </div>\r\n                <div>\r\n                    <h3>5</h3>\r\n                </div>\r\n                <div>\r\n                    <h3>6</h3>\r\n                </div>\r\n            </div>\r\n            </div>\r\n        );\r\n    } else if (props.teachers !== undefined) {\r\n        return (\r\n            <div style={{width: '100%'}}>\r\n                <Slider {...settings}>\r\n                    {props.teachers.map(elem => <CourseTeacherCard teacher={elem}/>)}\r\n                </Slider>\r\n            </div>\r\n        )\r\n    } else if (props.collection !== undefined){\r\n        return (\r\n            <div style={{width: '100%'}}>\r\n                <Slider {...settings} style={{justifyContent: 'space-between'}}>\r\n                    {props.collection.map(collectionItem